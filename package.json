    {
      "path": "dist/yt-chapters.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/ffprobe-static": "^2.0.0",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "execa": "^5.0.0",
    "ffprobe-static": "^3.0.0",
    "meow": "^9.0.0"
